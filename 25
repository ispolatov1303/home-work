c = 0
massiv = []

for n in range(222222, 666667):
    s = str(n)
    if int(s[0]) % 2 == 0 and int(s[1]) % 2 == 0:
        if n % 5 == 0 and n % 13 == 0 and n % 19 == 0:
            su = sum(int(d) for d in s)
            d = n // 1235
            massiv.append([su, d])

for i in massiv[-5:]:
    print(i[0], end = ' ')
    print(i[1])
'''26 535
28 536
30 537
23 538
34 539'''
